<?xml version="1.0" encoding="UTF-8"?>
<resources>
    <declare-styleable name="FaRecyclerView">
        <attr name="fa_pull_to_refresh" format="boolean"/>
        <attr name="fa_load_more" format="boolean" />
        <attr name="fa_progress_in_center" format="boolean" />
        <attr name="fa_orientation">
            <flag name="vertical" value="0x0"/>
            <flag name="horizontal" value="0x01000000"/>
        </attr>
        <attr name="fa_type">
            <flag name="list" value="0x0"/>
            <flag name="grid" value="0x01000000"/>
            <flag name="stagged" value="0x02000000"/>
        </attr>
        <attr name="fa_spanCount" format="integer" />

        <attr name="fa_layout_empty" format="reference"/>
        <attr name="fa_layout_moreProgress" format="reference"/>
        <attr name="fa_layout_progress" format="reference"/>
        <attr name="fa_recyclerClipToPadding" format="boolean"/>
        <attr name="fa_recyclerPadding" format="dimension"/>
        <attr name="fa_recyclerPaddingTop" format="dimension"/>
        <attr name="fa_recyclerPaddingBottom" format="dimension"/>
        <attr name="fa_recyclerPaddingLeft" format="dimension"/>
        <attr name="fa_recyclerPaddingRight" format="dimension"/>
        <attr name="fa_scrollbarStyle">
            <flag name="insideOverlay" value="0x0"/>
            <flag name="insideInset" value="0x01000000"/>
            <flag name="outsideOverlay" value="0x02000000"/>
            <flag name="outsideInset" value="0x03000000"/>
        </attr>
        <attr name="fa_vertical_scrollbarEnabled" format="boolean" />
        <attr name="fa_horizontal_scrollbarEnabled" format="boolean" />
        <attr name="fa_horizontal_wrap_content" format="boolean" />
        <attr name="fa_vertical_wrap_content" format="boolean" />

    </declare-styleable>

</resources>